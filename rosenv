#!/bin/bash
. /etc/lsb-release

ACCESS_OBC_THROUGH_AGV_INTERNAL_NETWORK=false
LOCAL_ROS_MASTER=true

export MY_HOME_PATH=/home/haibo
export ROS_DISTRO=indigo
export DEFAULT_DPL_NAME=cwt_4pl #ht_gazebo_sim mxa_gazebo_sim
#export DEFAULT_DPL_TYPE=server
export ROBOT_ID=10
export AGV_CONFIG=sesto_agv_forklift    #change accordingly
export BEHAVIOR_CONFIG=bidirectional_dwa    #change accordingly
export AGV_MODE=0 #0 = NAVIGATION, 1 = MAPPING, 2 = SYSTEM_CHECK, 3 = MONITORED_TEST, 4 = SCHEDULED_DOWN
export SMACH_CONFIG=smach_npoints
export CUSTOMER_CONFIG=ht_gazebo_sim
export MAP_CONFIG=sim_ht_office
export SERVER_IP=tswhopamhs.wlsg.micron.com #change accordingly
export SERVER_PORT=20002                    #change accordingly
export AGV_UI_IP=192.168.88.70
export AGV_UI="hope@"$AGV_UI_IP #Any changes to UI username has to update sesto-start

export DATA_LOGGING_PREFIX=$HOSTNAME
MAIN_REPOSITORY_PATH=~/catkin_ws/src/    # Change as appropriate

#### SOURCING OF CATKIN WORKSPACE ####
source /opt/ros/indigo/setup.bash
source $MY_HOME_PATH/catkin_ws/install/setup.bash

#### PACKAGES/LIBRARIES PATHS ####

# export ROS_PACKAGE_PATH=$MY_HOME_PATH/rosbuild-packages:$ROS_PACKAGE_PATH   
# This is if you need to use rosbuild packages that you keep in the $MY_HOME_PATH/rosbuild-packages folder

export LD_LIBRARY_PATH=/usr/local/lib:$MY_HOME_PATH/catkin_ws/install/lib:/opt/ros/$ROS_DISTRO/lib  

#### ROS CONSOLE FORMATTING ####
export ROSCONSOLE_FORMAT='${message}'             
# To remove the ROS time display in brackets in the terminal

#### GENERAL CPP LOGGING ####
export PATH_TO_LOGGING_CONF_FILE="/usr/local/micronES/logging.conf"

#### IPs and SSH ####
export OBCUSER=hope
export PICOUSER=hope

if [[ $ACCESS_OBC_THROUGH_AGV_INTERNAL_NETWORK == true ]]
then
	export OBCIP=192.168.88.10 #On-board AGV
	export OBCFWDPORTSSH='' 			
	export OBCFWDPORTSCP=''

	export PICOIP=192.168.88.20	
else
	export OBCIP=f10wsestoagv02
	export OBCFWDPORTSSH='-p 55555' 	
	export OBCFWDPORTSCP='-P 55555'

	export PICOIP=$OBCIP 
fi
				
export OBC=$OBCUSER@$OBCIP 			
alias seslog='ssh $OBCFWDPORTSSH $OBC'

#### ROS IPs ####
if [[ $LOCAL_ROS_MASTER == true ]]
then
	export ROS_IP=localhost
	export ROS_MASTER_URI=http://localhost:11311	
else
	if [[ $ACCESS_OBC_THROUGH_AGV_INTERNAL_NETWORK == true ]]
	then
		export ROS_IP=192.168.88.254  #On-board AGV  (cf "AGV subnet configuration")
	else
		export ROS_IP=192.168.0.240
	fi
	export ROS_MASTER_URI=http://$OBCIP:11311          
fi

export ROS_HOSTNAME=$ROS_IP	                  
# Should be the same as ROS_IP. It seems those parameters are redundant in certain cases but we add both for security


# NB : The parameters below are fake, they are only used when running 
# all nodes on the laptop

export LASER_1_IP=192.168.88.30
export LASER_2_IP=192.168.88.31
export LASER_3_IP=192.168.88.32

export USING_UXM=false

#### Data Logging Folder Path ###
export PATH_TO_DATA_LOGGING_FOLDER="/home/haibo/rosbags"
export PATH_TO_RAW_DATA_LOGGING=$PATH_TO_DATA_LOGGING_FOLDER/localrecording

#### Generate bash alias for all the scripts in ht_sesto_utils/ht_common/scripts ####
make_aliases_for_scripts_in_folder(){ # $1 = folder_path
	cur_dir=`pwd`
	cd $1
	for script_path in *.bash
	do
		script_name=$(basename $script_path .bash)
		alias $script_name=`pwd`"/"$script_path
	done
	cd $cur_dir
}
export SCRIPTS_FOLDER=$MAIN_REPOSITORY_PATH'ht_sesto_utils/ht_common/scripts/'
make_aliases_for_scripts_in_folder $SCRIPTS_FOLDER
#export CUSTOM_CATKIN_WHITELIST='sesto_controller;sesto_rotate_recovery;sesto_nav;sesto_msgs;payload_msgs;sesto_server_interface;agv_ui_interface;sesto_agv_status_manager;payload_manager;payload_controller;log_translator;ros_routing;msg_encoding_utils;rosbag_recorder;rosparam_utils;rviz_utils;serial_line;socket_comms;amcl;carrot_planner;dwa_local_planner;move_base;nav_core;base_local_planner;clear_costmap_recovery;fake_localization;global_planner;move_base_msgs;navfn;rotate_recovery;costmap_2d;map_server;move_slow_and_clear;robot_pose_ekf;voxel_grid;custom_costmap_layers;pose_follower;simple_interpolation_route_planner;laser_filters;notification_manager;map_streamer;smach_viewer;laser_scan_splitter;ira_laser_tools;sound_play;hector_compressed_map_transport;hector_geotiff;hector_geotiff_plugins;hector_imu_attitude_to_tf;hector_map_server;hector_map_tools;hector_mapping;hector_marker_drawing;hector_nav_msgs;hector_slam_launch;hector_trajectory_server;slam_karto;open_karto;light_manager;graph_localization_manager;navplan_parser;sesto_utils;kvh_dsp3000;openslam_gmapping_ros;openslam_gmapping_lib;hector_mapstitch;odom_publisher;sesto_map_manager;sesto_dpl;file_ops;xmlrpc_utils;routing_graph_manager;ht_comms;ht_common;system_utils;msg_encoding_utils;rosparam_utils;payload_msgs;sesto_msgs;ar_docker;ar_track_alvar;docking_controller;msg_encoding_utils;payload_manager;payload_comms_handler;payload_controller;msg_encoding_utils;brooks_atr60lf_rfid;laser_lines';
